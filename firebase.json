{
  "hosting": {
    "public": "dist/pdcg/browser",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/assets/robots.txt"
      },
      {
        "source": "/sitemap.xml",
        "destination": "/assets/sitemap.xml"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
